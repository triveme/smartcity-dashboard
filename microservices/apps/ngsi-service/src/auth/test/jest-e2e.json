{
    "moduleFileExtensions": ["js", "json", "ts"],
    "rootDir": ".",
    "testEnvironment": "node",
    "testRegex": ".e2e-spec.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "moduleNameMapper": {
      "@app/postgres-db/(.*)": "<rootDir>/../../../libs/postgres-db/src/$1",
      "@app/postgres-db": "<rootDir>/../../../libs/postgres-db/src"
    }
  }
  